<Group labelreference="L355" drawlabel="true">
<Input>
<Text-field style="Text" layout="Normal">Initialize routines</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">hartree2eV:=simplify(Unit('hartree/eV'));
bohr2A:=simplify(Unit('bohr/Ao'));

len:=p-&gt;evalf(sqrt(add(v^2,v=p))):

angle:=(p1,p2)-&gt;evalf(arccos(add(p1[i]*p2[i],i=1..3)/len(p1)/len(p2))*180/Pi):

ReadAtoms:=proc(lsnames::list,filename::string:=out,$)
local n,fd,ls,Atoms,i,e;
n:=nops(lsnames);
fd:=fopen(filename,READ,TEXT);
ReadValue(fd,key=&quot;outvars: echo values of variables after computation&quot;);
ls:=ReadLines(fd,'key'=&quot;xangst&quot;,'shift'=0,'nlines'=n);
fclose(fd);
ls[1]:=StringTools[Substitute](ls[1],&quot;xangst&quot;,&quot;&quot;);
Atoms:=table():
for i from 1 to n do
 e:=sscanf(ls[i],&quot;%f%f%f&quot;);
 if (nops(e)=3) then Atoms[i]:=[lsnames[i],&lt;e[1],e[2],e[3]&gt;] else error &quot;Unrecognized xangst entry, %1&quot;,ls[1] end end;
[seq(Atoms[i],i=1..n)]
end:

ReadNumbers:=proc(key::string,format::string:=&quot;%e&quot;,filename::string:=out,$)
local fd,E;
fd:=fopen(filename,READ,TEXT);
ReadValue(fd,':-key'=&quot;outvars: echo values of variables after computation&quot;);
E:=ReadValue(fd,':-key'=key,'relpos',':-format'=format):
fclose(fd);
E
end:

ReadTime:=proc(filename::string:=out,$) ReadValue(filename,'key'=&quot;Overall time at end (sec) : cpu=&quot;,'relpos','format'=&quot;%f&quot;) end:

ReadMessageSCF:=proc(filename::string:=out,$) ReadValue(filename,'key'=&quot;At SCF step&quot;) end:

ReadMessageOpt:=proc(filename::string:=out,$)
local e;
e:=ReadValue(filename,'key'=&quot;At Broyd/MD step&quot;);
if (e=NULL) then
 e:=ReadLines(filename,'key'=&quot;was not enough Broyd/MD steps to converge gradients&quot;,'shift'=0,nlines=2);
<Font encoding="UTF-8"> cat(e[1],&quot;\134n&quot;,e[2])
</Font>else
 e end
end:</Text-field>
</Input>
<Output>
<Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display="">JCIwc15iY1I2cyMhIzg=</Equation></Text-field>
</Output>
<Output>
<Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display="">JCIwPHBdZHM8SCYhIzo=</Equation></Text-field>
</Output>
