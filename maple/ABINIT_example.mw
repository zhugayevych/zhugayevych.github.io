<Text-field prompt="&gt; " style="Maple Input" layout="Normal">fd:=fopen(inp,WRITE,TEXT):
<Font encoding="UTF-8">fprintf(fd,&quot;\134# %s molecule\134n\134n&quot;,nam):
</Font>fprintf(fd,&quot;ntypat 2<Font encoding="UTF-8">\134nznucl %d 1\134nnatom 6\134ntypat 1 1 2 2 2 2\134nxangst\134n</Font>&quot;,Z):
for Atom in Atoms do <Font encoding="UTF-8">fprintf(fd,&quot; %12.8f\134n&quot;,Atom[2]</Font>) end:
fprintf(fd,&quot;acell %.1f %.1f %.1f<Font encoding="UTF-8"> angstrom\134n\134n&quot;,R,R,R+d0):</Font>
<Font encoding="UTF-8">fprintf(fd,&quot;ixc %d\134necut %.0f\134nkptopt 0\134nnkpt 1\134nnstep %d\134ntoldff %.1e\134ndiemac 2\134n\134n</Font>&quot;,ixc,ecut,nstep,toldff):
fprintf(fd,&quot;<Font encoding="UTF-8">ionmov 3\134nntime %d\134ntolmxf %.1e\134n</Font>&quot;,ntime,tolmxf):
fclose(fd);</Text-field>
